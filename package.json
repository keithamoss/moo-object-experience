{
  "name": "moo-object-experience",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "tsc --noEmit && biome check --diagnostic-level=error .",
    "lint:fix": "biome check --write --unsafe .",
    "format": "biome format --write .",
    "test": "tsc --noEmit && vitest --run",
    "test:report": "vitest --run; npx vite preview --outDir testing/vitest-report",
    "test:watch": "vitest",
    "test:ui": "vitest --ui --run",
    "test:coverage": "vitest --coverage --run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report testing/playwright-report"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.18",
    "@mui/system": "^5.15.15",
    "@reduxjs/toolkit": "^2.2.5",
    "minisearch": "^7.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet-async": "^1.0.3",
    "@types/react-redux": "^7.1.33",
    "@vitejs/plugin-basic-ssl": "^2.1.4",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "jsdom": "^25.0.1",
    "typescript": "^5.4.5",
    "vite": "^6.0.5",
    "vite-plugin-checker": "^0.8.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^4.0.18"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "npm@10.0.0"
}